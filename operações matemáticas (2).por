programa {
  funcao inicio() {
    //Declaração de variáveis 
    inteiro n1, n2

    //Inicio de código
    escreva ("Insira o primeiro valor: ")
    leia (n1)
    escreva ("Insira o primeiro valor: ")
    leia (n2)

    escreva ("O resultado das 4 operações a seguir: ")
    escreva("\nSoma: ", n1 + n2)
    escreva("\nSubtração: ", n1 - n2)
    escreva("\nDivisão: ", n1 / n2)
    escreva("\nMultiplicação: ", n1 * n2)

  }
}